# dotfiles
alias als="vim ~/github.com/nathanpanics/dotfiles/.aliases"
alias dfs="cd ~/github.com/nathanpanics/dotfiles"
alias zshrc="vim ~/github.com/nathanpanics/dotfiles/.zshrc"

# git
alias gaas="git add -u && git reset -- \*.swp"

# navigation
alias ggi="cd ~/github.com/getgrowit"

# shell
alias coc="vim ~/.vim/coc-settings.json"
alias tmux="TERM=xterm-256color tmux"
alias vmrc="vim ~/.vim/vimrc"
